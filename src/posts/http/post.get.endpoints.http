// segunda, dinamica
{{ 
  // Pre-request script
  const endDate = new Date();
  const startDate = new Date();
  startDate.setDate(endDate.getDate() - 1);
  
  // Usar process.env si httpYac lo soporta
  process.env.START_DATE = startDate.toISOString().split('T')[0];
  process.env.END_DATE = endDate.toISOString().split('T')[0];
}}

GET http://localhost:3000/posts/11?startDate={{process.env.START_DATE}}&endDate={{process.env.END_DATE}}&limit=1&page=3



